
<view style='width:100%;height:100%;background:#f1f1f1;position:relative;padding-bottom:20rpx;'>
<view class="wrap">
    <view class='div2' data-id='{{item.id}}' bindtap='xiang' wx:for="{{zhi}}">
    <view  >
         <view class='div21' style='overflow:hidden;border-radius:10rpx;'><image src="{{img}}{{item.url}}" class="{{item.width >= item.height ? 'gao' : 'kuan'}}" ></image></view>
         <text class='div22'>{{item.title}}</text>
         <text class='div23'>{{item.intime}}</text>
         </view>
    </view>
   
</view>
</view>